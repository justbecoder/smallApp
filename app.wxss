/* app.wxss针对整个小程序 */
page{
  width: 750rpx;
  background: #e1e1e1;
}

/* 定义回到顶部按钮的样式 */
#backToTop{
  width: 20px;
  color: gray;
  padding: 12px 5px;
  border: 1px solid #CCC;
  background: #f5f5f5;
  position: fixed;
  bottom: 120px;
  right: 0;
  border-radius: 12px 0px 0px 12px;
  font-size: 14px;
  text-align: center
}